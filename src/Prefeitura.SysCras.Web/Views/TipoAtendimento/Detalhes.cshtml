@model TipoAtendimentoViewModel

@{ 
    ViewData["Title"] = "Detalhes do Tipo de Atendimento";
}

<div class="container">
    <h5 class="text-secondary">
        <i class="fas fa-comment-alt"></i>
        Tipo Atendimento
    </h5>
    <div class="table-overflow">
        <table class="table table-striped table-sm">
            <thead>
                <tr>
                    <th>Código</th>
                    <th>Tipo</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@Model.Id</td>
                    <td>@Model.Tipo</td>
                </tr>
            </tbody>
        </table>
        <a class="btn btn-sm btn-primary btn-form mr-2" asp-action="Index" title="Index">
            <i class="fas fa-caret-left"></i> Voltar
        </a>
        <a class="btn btn-sm btn-success btn-form mr-2" asp-action="Atualizar" asp-route-id="@Model.Id" title="Atualizar">
            <i class="fas fa-edit"></i> Atualizar
        </a>
        <a class="btn btn-sm btn-danger btn-form mr-2" asp-action="Excluir" asp-route-id="@Model.Id" title="Excluir">
            <i class="fas fa-trash-alt"></i> Excluir
        </a>
    </div>
</div>
